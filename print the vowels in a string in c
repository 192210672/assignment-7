#include <stdio.h>
#include <string.h>

int main() {
    char string[100];
    int length, i;

    printf("Enter a string: ");
    scanf("%s", string);

    length = strlen(string);

    printf("Vowels in the string: ");

    for(i=0; i<length; i++) {
        if(string[i]=='a' || string[i]=='e' || string[i]=='i' || string[i]=='o' || string[i]=='u' ||
           string[i]=='A' || string[i]=='E' || string[i]=='I' || string[i]=='O' || string[i]=='U') {
            printf("%c ", string[i]);
        }
    }

    return 0;
}
